point-tracker{text-align:center}point-tracker h2{margin:0;margin-bottom:4px}point-tracker div.container{width:82px;margin:auto}point-tracker editable-number{padding:20px;font-size:20px;font-weight:black;border:1px solid #000;width:40px;margin-bottom:5px;border-radius:20%}point-tracker button{font-size:18px;font-weight:700;padding:5px 10px 5px 10px;background-image:radial-gradient(circle at center,#ddd 50%,#ccc 100%);width:48%;-webkit-box-shadow:0 1px #333;box-shadow:0 1px #333;border:0;border-radius:15%}point-tracker .button-container{margin:auto;position:relative;overflow:auto;height:40px}point-tracker .button-left{position:absolute;left:0}point-tracker .button-right{position:absolute;right:0}