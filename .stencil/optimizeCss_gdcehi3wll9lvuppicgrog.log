character-skill > div {
  border: 1px solid black;
  padding: 5px;
  display: -ms-flexbox;
  display: flex;
  min-width: 150px;
  width: 150px;
  border-collapse: collapse; }

character-skill div > div {
  margin: auto; }

character-skill .is-proficient {
  -ms-flex: .2;
  flex: .2; }

character-skill .current-bonus {
  -ms-flex: .5;
  flex: .5; }

character-skill .name {
  -ms-flex: 1;
  flex: 1;
  text-align: left; }

character-skill #isProficient.proficient::before {
  content: 'x'; }

character-skill #isProficient {
  display: inline-block;
  height: 20px;
  width: 10px;
  border: 1px black dashed; }